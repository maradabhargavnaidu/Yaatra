{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off"
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    }
}
